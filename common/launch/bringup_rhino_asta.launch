<launch>
    <rosparam file="$(find common)/config/vehicles/rhino/config/car.yaml" />

    <param name="use_sim_time" value="true"/>
    <arg name="fssim_config" default="$(find common)/config/sim_config/sim_rhino_asta.yaml"/>

    <include file="$(find fssim)/launch/start.launch">
        <arg name="fssim_config" value="$(arg fssim_config)"/>
    </include>

    <node name="rviz_" pkg="rviz" type="rviz" args="-d $(find common)/config/rviz_config/saarti_sim.rviz "/>

</launch>
