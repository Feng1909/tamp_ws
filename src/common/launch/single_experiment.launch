<!-- Runs single experiment. Experiment configuration in: single_experiment_config.yaml-->

<launch>
     <!-- params -->
     <param name="use_sim_time" value="true" />
     <rosparam command="load" file="$(find common)/config/single_experiment_config.yaml" />
     <rosparam command="load" file="$(find vehicle_models)/config/static_params_vehicle1.yaml" />

     <!-- experiment manager -->
     <node pkg="common" type="experiment_manager.py" name="experiment_manager" output="screen" required="true"> </node>

     <!-- vehicle, localization, object detect -->
     <include file="$(find common)/launch/base.launch" />

     <!-- planner node
     <node pkg="planning" type="motionplanner_node" name="saasqp"> </node>
     -->
</launch>
