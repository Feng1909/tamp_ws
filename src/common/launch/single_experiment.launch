<!-- Runs single experiment. Experiment configuration in: single_experiment_config.yaml-->

<launch>
     <!-- params -->
     <param name="use_sim_time" value="true" />
     <rosparam command="load" file="$(find common)/config/single_experiment_config.yaml" />
     <rosparam command="load" file="$(find vehicle_models)/config/static_params_vehicle1.yaml" />

     <!-- experiment manager -->
     <node pkg="common" type="experiment_manager.py" name="experiment_manager" output="screen" required="true"> </node>

     <!-- vehicle, localization, object detect -->
     <node pkg="common" type="dummy_trajhat_publisher.py" name="dummy_trajhat_publisher"> </node>
     <node pkg="perception" type="localization_and_state_est.py" name="localization_and_state_est" output="screen"> </node>
     <node pkg="perception" type="object_detection.py" name="object_detection"> </node>
     <node pkg="vehicle_models" type="simple_vehicle.py" name="simple_vehicle"> </node>

     <!-- planner and control interface node -->
     <node pkg="planning" type="motionplanner_node" name="saasqp"> </node>
     <node pkg="planning" type="ctrl_interface.py" name="ctrl_interface"> </node>

</launch>
